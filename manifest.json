{
    "name": "Web Video Transcript",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["css/index.css"],
            "js": ["js/button_start.js"]
        }
    ],
    "browser_action": {
        "default_popup": "button.html",
        "default_title": "Video Transcript"
    }
}